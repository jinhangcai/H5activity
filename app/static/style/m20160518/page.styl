@import "../common.styl";
@import "../address_moudle";
//阴影边框
frame=@block{
 width 6rem
 border-radius .2rem
 -webkit-box-shadow:5px -5px 5px #c94b66 inset;
 box-shadow:5px -5px 5px #c94b66 inset;
}
//小框大小
frame-box=@block{
   width 5.7rem
   position absolute
   left .15rem
   top .15rem
   border-radius .2rem
   -webkit-box-shadow:-2px 2px 2px #e9d4d6 inset;
   box-shadow:-2px 2px 2px #e9d4d6 inset;
}
//图片定位
imgs=@block{
  position absolute
  width 3.43rem
  height 1.77rem
  left 1.3rem
  top -.93rem
  z-index 2
  img{
    width 100%
  }
}
//字体大小
Font-size=@block{
  font-size .26rem
  color #333333
  padding-left .22rem
  span{
   color #c2262c
  }
}
.box{
  background #1d96d7
  .banner {
    width 100%
    position relative
    .floatfix{
      width 1.2rem
      height 1.2rem
      position absolute
      right 0
      top 0
    }
    img{
      width 100%
    }
  }
}
.box-cen{
  width 6rem
  margin 0 auto
  margin-top .92rem
  .frmae{
    position relative
    height 3.32rem
    margin-bottom .22rem
    background -webkit-linear-gradient(#e67997, #d8486d)
    background linear-gradient(#e67997, #d8486d)
    {frame}
    .highlight{
      position: absolute;
      right: 0;
      top: 0;
      width: 1.22rem;
      height: 0.92rem;
      background: -webkit-linear-gradient(right top, rgba(255,255,255,.5) , rgba(255,255,255,0)); /* Safari 5.1 - 6.0 */
      background: linear-gradient(to left bottom, rgba(255,255,255,.5) , rgba(255,255,255,0)); /* 标准的语法 */
      border-radius .2rem
      opacity .3
     }
    }
    .redbank{
      {imgs}
    }
    .frame-box{
      height 3rem
      background #fceff1
      {frame-box}
      .title{
        {Font-size}
      }
      .title:nth-child(1){
        padding .9rem 0 .3rem .22rem
      }
    }
    .shopuls{
      overflow hidden
      width 5.3rem
      margin .4rem auto 0
      li{
        width 2.5rem
        height 2rem
        margin 0 .3rem .4rem 0
        float left
        .li-frame{
          width 2.5rem
          height 1.7rem
          background #f8f2de
          border-radius .2rem
          img{
            width 100%
          }
        }
        .price{
          color #333333
          font-size .2rem
          text-align center
          padding-top .1rem
        }
       .handline{
         color #9d4c1b
         font-size .2rem
         text-align center
       }
      }
      li:nth-child(even){
        margin 0 0 .4rem 0
      }
    }
    .pricelist{
      font-size .26rem
      color #9d4c1b
      margin-top .4rem
      text-align center
    }
    .dislist{
      width 4.15rem
      height 3.1rem
      margin .1rem auto 0
      background #fab537
      border-radius .1rem
      margin-bottom .4rem
      .dislist-ul{
        overflow hidden
        width 4.09rem
        height auto
        margin 0 auto
        padding-top .05rem
        li{
          font-size .22rem
          color #333
          height .47rem
          background #fffbec
          margin-bottom .03rem
          .dislist-left{
            width 50%
            line-height .47rem
            text-align center
            float left
          }
          .dislist-right{
            width 50%
            line-height .47rem
            text-align center
            float right
          }
        }
        li:nth-child(1){
          font-size .26rem
          background none
          height .51rem
          line-height .51rem
        }
        li:nth-child(6){
         border-radius 0 0 .1rem .1rem
        }
      }
    }
  }
  .frmae:nth-child(3){
    height 16.75rem
    background -webkit-linear-gradient(#fdc461, #f8ae25)
    background linear-gradient(#fdc461, #f8ae25)
    -webkit-box-shadow:5px -5px 5px #e7b54a inset;
    box-shadow:5px -5px 5px #e7b54a inset;
    .frame-box{
      background #fffbec
      height auto
    }
    .titlein{
      width 5.3rem
      {Font-size}
      margin .9rem auto 0
    }
  }
  .star{
    width 100%
    img{
      width 100%
    }
  }

  .box-cen .general{
    height 2.5rem
    background -webkit-linear-gradient(#84dc78, #56ce49)
    background linear-gradient(#84dc78, #56ce49)
    -webkit-box-shadow:5px -5px 5px #4ebe47 inset;
    box-shadow:5px -5px 5px #4ebe47 inset;
    .frame-box{
      background #eff6ef
      height 2.2rem
      -webkit-box-shadow: -2px 2px 2px #cee0ce inset;
      box-shadow: -2px 2px 2px #cee0ce inset;
    }
    .titlein{
      width 5.3rem
      {Font-size}
      margin .9rem auto 0
      span{
        color #2b812b
      }
    }
 }
.box-cen .ticket{
  height 4.67rem
  background -webkit-linear-gradient(#60e3ff, #23cffa)
  background linear-gradient(#60e3ff, #23cffa)
  -webkit-box-shadow:5px -5px 5px #3cc0e7 inset;
  box-shadow:5px -5px 5px #3cc0e7 inset;
  .frame-box{
    background #f4fcff
    height 4.37rem
    -webkit-box-shadow: -2px 2px 2px #d3e5ee inset;
    box-shadow: -2px 2px 2px #d3e5ee inset;
    .headbom{
      width 5.17rem
      height 1.43rem
      margin 0 auto
      margin-top .4rem
      background #ddf6fd
      border-radius .1rem
      .headbom-title{
        {Font-size}
        color #0b7eab

        padding .27rem 0 0 .25rem
      }
    }
  }
  .titlein{
    width 5.3rem
    {Font-size}
    margin .9rem auto 0
  }
}
.Short{
  color #fff
  font-size .28rem
  padding .12rem 0 .25rem 0
  text-align center
}
.introduce{
  padding-bottom 2rem
  li{
    {Font-size}
    color #fff
    padding-top .25rem
    .bbs{
      color #fdff69
      text-decoration underline
    }
  }
}
#btnrun{
  display none
  width 100%
  height .88rem
  background -webkit-linear-gradient(#97f2ff, #28c3e4)
  background linear-gradient(#97f2ff, #28c3e4)
  text-align center
  line-height .88rem
  font-size .3rem
  color #0c5f8c
  position fixed
  left 0
  bottom 0
  font-weight bold
  z-index 3
}
.poker{
  display none
  position absolute
  z-index 5
  left 0
  top 0
  width 100%
  height 100%
  background-color rgba(0,0,0,0.6)
  .frmae{
    .highlight{
      position: absolute;
      right: 0;
      top: 0;
      width: 1.22rem;
      height: 0.92rem;
      background: -webkit-linear-gradient(right top, rgba(255,255,255,.5) , rgba(255,255,255,0)); /* Safari 5.1 - 6.0 */
      background: linear-gradient(to left bottom, rgba(255,255,255,.5) , rgba(255,255,255,0)); /* 标准的语法 */
      border-radius .2rem
      opacity .3
    }
    position relative
    height 5.22rem
    margin-top 2.5rem
    background -webkit-linear-gradient(#60e3ff, #24cffb)
    background linear-gradient(#60e3ff, #24cffb)
    -webkit-box-shadow:5px -5px 5px #39c2eb inset;
    box-shadow:5px -5px 5px #39c2eb inset;
    .close{
      width .65rem
      height .65rem
      position absolute
      right -.2rem
      top -.2rem
      z-index 6
    }
    .frame-box{
      height 4.88rem
      background #ecf9ff
      -webkit-box-shadow: -2px 2px 2px #d0e4ee inset;
      box-shadow: -2px 2px 2px #d0e4ee inset;
    }
    .redbank{
      top -.22rem
    }
    .titlein{
      width 4.8rem
      margin 0 auto
      padding .8rem 0 0
      color #333333
      font-size .24rem
    }
    .titlein:nth-child(2){
      padding .4rem 0 0 0
    }
  }

}
#headbomone{
  width 5rem
  height 5rem
  background #999
  margin 0 auto
  #canvas{
    width 100%
    height 100%
  }
}