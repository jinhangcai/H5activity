@import "../common.styl";
.viewport{
  background #060c1e
}
.banner{
  width 100%
  img{
    width 100%
  }
}
.main{
  width 5.56rem
  margin 0 auto
  overflow hidden
  .title{
    width 100%
    img{
      width 3.1rem
      margin .33rem auto 0
      display block
    }
  }
  .titletwo{
    margin-top .8rem
  }
  .handline{
    font-size .24rem
    color #fff
    padding-top .26rem
  }
  .team{
    width 5.3rem
    margin .22rem auto .2rem
    background #0f1c3a
    text-align center
    line-height .9rem
    height .9rem
    font-size .28rem
    color #edb92c
  }
  .record{
    color #edb92c
    font-size .24rem
    width 5.3rem
    height .5rem
    line-height .5rem
    background #0f1c3a
    text-align center
    margin 0 auto .4rem
    a{
      color #edb92c
      font-size .28rem
      margin-right .6rem
      text-decoration underline
    }
    a:nth-last-child(1){
      margin-right 0
    }
  }
  .gotologin{
    width 3.1rem
    margin .32rem auto .3rem
    display block
    img{
      width 100%
    }
  }
  .rules{
    width 100%
    text-align center
    a{
      color #fff
      font-size .24rem
      text-decoration underline
    }
  }
  .login{
    width 100%
    background #0f1c3a
    color #edb92c
    font-size .24rem
    text-align center
    margin-top .3rem
    line-height 250%
    span{
      color #edb92c
      font-size .24rem
      text-decoration none
    }
    .login-dr{
      text-decoration underline
    }
  }
  .shop{
    overflow hidden
    li{
      width 50%
      float left
      margin-top .4rem
      img{
        display block
        width 1.59rem
        margin 0 auto
      }
      .redbom{
        width 1.72rem
        margin-bottom .19rem
      }
      p{
        text-align center
      }
      p:nth-child(2){
        color #edb92c
        font-size .22rem
        padding-top .2rem
      }
      p:nth-child(3){
        font-size .22rem
        color #fff
        padding-top .1rem
      }
    }
    li:nth-child(1){
      width 100%
    }
  }
  .instructions{
    padding-top .45rem
    font-size .22rem
    color #fff
  }
  .ols{
    width 100%
    overflow hidden
    padding-bottom .58rem
    li{
      font-size .22rem
      color #fff
      padding-top .18rem
      line-height .3rem
    }
  }
}
.mask{
  position: fixed
  z-index 10
  top 0
  left 0
  width 100%
  height 100%
  background-color rgba(0,0,0,0.7)
  .pop{
    box-sizing: border-box;
    position: absolute;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    width: 4.5rem;
    .pop-top,.pop-bottom{
      width 100%
      height .26rem
      overflow hidden
    }
    .pop-top{
      background url("../../images/m20170118/pop-top.png") no-repeat
      background-size 100%
    }
    .pop-bottom{
      background url("../../images/m20170118/pop-bottom.png") no-repeat
      background-size 100%
      margin-top -0.02rem
    }
    .cen{
      background url("../../images/m20170118/pop-tile.png") repeat
      background-size 100%
      width 100%
      padding-bottom .15rem
      margin-top -0.02rem
      .cen-main{
        width 3.97rem
        margin 0 auto
        h2{
          color #be0611
          font-size .32rem
          margin 0 auto
          text-align center
          padding-top .2rem
        }
        p{
          color #060c1e
          font-size .24rem
          padding-top .38rem
        }
        p:nth-child(2){
          padding-top .2rem
        }
        .cen-btn{
          width 2.5rem
          height .7rem
          display block
          margin .57rem auto 0
          background url("../../images/m20170118/know.png") no-repeat
          background-size 100%
        }
        .blessing{
          width 2.14rem
          height 2.22rem
          display block
          margin 0 auto
        }
        .img1{
          background url("../../images/m20170118/fghz.jpg") no-repeat
        }
        .img2{
          background url("../../images/m20170118/cftx.jpg") no-repeat
        }
        .img{
          display block
          width 2.76rem
          height 3.12rem
          margin .27rem auto 0
          background-size 100%
        }
        .img-main{
          width 100%
          margin 0 auto
          padding-top .27rem
        }
      }
    }
  }
}
.pk{
  width 5.31rem
  margin .25rem auto 0
  overflow hidden
  position relative
  .bg{
    width 2.31rem
    float left
    .titleimg{
      width 1.85rem
      display block
      margin .29rem  auto .15rem
    }
    .winning{
      width 100%
      margin-bottom .42rem
      .hztop{
        background url("../../images/m20170118/redborder-top.png") no-repeat
      }
      .hzcen{
        background url("../../images/m20170118/redborder-cen.png") repeat
      }
      .hzbottom{
        background url("../../images/m20170118/redborder-bottom.png") no-repeat
      }
      .cftop{
        background url("../../images/m20170118/blueborder-top.png") no-repeat
      }
      .cfcen{
        background url("../../images/m20170118/blueborder-cen.png") repeat
      }
      .cfbottom{
        background url("../../images/m20170118/blueborder-bottom.png") no-repeat
      }
      .top,.bottom{
        width 100%
        height .2rem
        background-size 100%
      }
      .cen{
        width 100%
        background-size 100%
      }
      .cen-main{
        width 1.9rem
        margin 0 auto
        .investment{
          font-size .24rem
          color #edb92c
          text-align center
        }
        .money{
          font-size .32rem
          text-align center
          color #fff
        }
        h3{
          font-size .22rem
          text-align center
          padding-top .36rem
          color #edb92c
        }
        .ranking{
          width 100%
          text-align center
          padding-top .15rem
          p{
            text-align center
            span{
              font-style oblique
              font-size .26rem
              color #fff
              padding-right 0.1rem
            }
            font-size .22rem
            color #fff
          }
        }
      }
    }
  }
  .bg1{
    margin-right .68rem
  }
  .refresh{
    width .92rem
    height .92rem
    position absolute
    left 41.2%
    top 42%
  }
  .fighting{
    position absolute
    left 42.5%
    top 0.45rem
    p{
      color #fff
      font-size .22rem
      text-align center
    }
  }
}
.win{
  background-color rgba(6,12,30,1)
  .win-title{
    color #fff
    font-size .28rem
    text-align center
    padding-top .8rem
  }
  .img-main{
    width 100%
    margin-top .2rem
    .img1{
      background url("../../images/m20170118/hz.png") no-repeat
    }
    .img2{
      background url("../../images/m20170118/cf.png") no-repeat
    }
    .img{
      width 2.65rem
      height .66rem
      margin 0 auto
      display block
      background-size 100%
    }
  }
  .redbom{
    color #fff
    font-size .28rem
    text-align center
    padding-top .35rem
    span{
      color #edb92c
    }
  }
  .fighting{
    top 10%
  }
  .pk{
    .cen-main{
      width 2.05rem !important
    }
    .investment{
      padding-top .1rem !important
    }
    .money{
      padding-top .1rem !important
    }
    h3{
      padding-top .2rem !important
    }
    .ranking{
      padding-top 0 !important
      .ranking-reward{
        width 100%
        overflow hidden
        p{
          float left
        }
        p:nth-child(1){
          width 1.5rem
          padding-top .17rem
        }
        p:nth-child(2){
          font-size .2rem
          color #be0611
          padding-top .2rem
          float right
        }
      }
      .ranking-moeny{
        width 100%
        overflow hidden
        p{
          float left
        }
        p:nth-child(1){
          width 1.5rem
          padding-top 0
        }
        p:nth-child(2){
          font-size .2rem
          color #be0611
          padding-top 0
          float right
        }
      }
    }
  }
  .pop{
    width 5.6rem
    top  37% !important
    .win-btn{
      width 2.5rem
      height .7rem
      display block
      margin .57rem auto 0
      background url("../../images/m20170118/know.png") no-repeat
      background-size 100%
    }
  }
  .pops{
    margin: 0 auto;
    box-sizing: border-box;
    width: 5.6rem;
    position: relative;
    .win-btn{
      width 2.5rem
      height .7rem
      display block
      margin .57rem auto 0
      background url("../../images/m20170118/know.png") no-repeat
      background-size 100%
    }
  }
}
.returnbg{
  .ranking{
    span{
      padding-right .1rem
    }
  }
}

#uiPopFrame{
  position fixed
  left 0
  top 0
  width 100%
  height 100%
  z-index 99999
}
.ui-pop-frame{
  .ui-pop-main{
    position absolute
    left 50%
    top 50%
    border-radius 0.1rem
    overflow hidden
    -webkit-transform translate(-50%, -50%)
    transform translate(-50%, -50%)
    width 5.3rem
    background-color #fff
    .ui-pop-cont{
      text-align: center !important;
      color: #666;
      font-size: 0.28rem !important;
      padding: 0.7rem 0.35rem;
      line-height: 1.5;
    }
    .ui-pop-operation{
      border-top: 1px solid #ccc;
      display: table;
      width: 100%;
      height: 100%;
      table-layout: fixed;
      a{
        display: table-cell;
        text-align: center;
        line-height: 0.9rem;
        font-size: 0.32rem;
        color: #f07e68;
      }
    }
  }
}
.ui-pop-wait {
  position: absolute
  width: 100%
  height: 100%
  .ui-pop-main {
    position: absolute
    left: 50%
    top: 50%
    border-radius .1rem;
    overflow: hidden
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    padding: .3rem;
    font-size: .28rem;
    background-color rgba(255,255,255,1);
    color:#000;
  }
}
.ui-pop-wait {
  position: absolute;
  width: 100%;
  height: 100%;
}
.ui-pop-wait .ui-pop-main {
  position: absolute;
  left: 50%;
  top: 50%;
  border-radius: .25rem
  overflow: hidden;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  padding: .3rem;
  font-size: .24rem
  color: #fff;
  background-color: rgba(0,0,0,0.7);
}