@import "../common.styl";
@import "../iconfont";
@font-face {
  $font-face(fzxs12, '../iconfont/fzxs12/fzxs12');
}
@font-face {
  $font-face(iconfont, '../iconfont/iconfont');
}
.iconfont {
  font-family: 'iconfont' !important;
  font-weight: 400;
  font-style: normal;
  font-variant: normal;
  line-height: 1;
  text-transform: none;
  speak: none;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-close:before{
  content: "\e604";
}

body{
  background-color #b4e6ff;
}

.viewport{
  height 100%;
  overflow auto;
  -webkit-overflow-scrolling: touch;
  overflow-scrolling: touch;
}
.banner{
  img {
    display: block;
    width 100%;
  }
}
.under-line{
  border-bottom 1px solid;
}
.high-light{
  color #ff1a24
}
.floor1{
  margin-top -.2rem;
  &:after{
    height .99rem;
    background url("data:image/png;base64,") no-repeat 0 0;
    background-size 100%;
    bottom -1.78rem
  }
}
.floor2{
  &:after{
    height 1.94rem;
    background url("data:image/png;base64,") no-repeat 0 0;
    background-size 100%;
    bottom -2.13rem
  }
}
.floor3{
  &:after{
    height 1.91rem;
    background url("data:image/png;base64,") no-repeat 0 0;
    background-size 100%;
    bottom -1.76rem
  }
}
.floor4{
  &:after{
    height 1.56rem;
    background url("data:image/png;base64,") no-repeat 0 0;
    background-size 100%;
    bottom -2.22rem
  }
}
.floor{
  &:after{
    content: '';
    position absolute;
    pointer-events none;
    left 0;
    width 100%;
  }
  position relative;
  margin-bottom 1.8rem;
  text-align center;
  .title{
    position relative;
    display inline-block;
    height .92rem;
    text-align center;
    &:before{
      content '';
      position absolute;
      top .03rem;
      left -.45rem;
      display block;
      width .72rem;
      height .72rem;
      background-color #fece02;
      border-radius 50%;
    }
    &:after{
      content '';
      position absolute;
      top 0;
      right -.58rem;
      display block;
      width .89rem;
      height .92rem;
      background url("data:image/png;base64,") no-repeat 0 0;
      background-size 100%;
    }
    h2 {
      position relative;
      z-index 2;
      font-size .4rem;
      font-weight bold;
      color #ff1a24;
      line-height .8rem;
    }
  }
  .info{
    margin 0 .3rem;
    text-align left;
    line-height .42rem;
    font-size .28rem;
    color #333;
    & + .info{
      margin-top .12rem;
    }
  }
}
.machine{
  box-sizing border-box;
  padding-top 2rem;
  height 5.74rem;
  background url(../../images/m20161218/machine.png) no-repeat 0 0;
  background-size 100%;
  .content{
    //display -webkit-flex;
    //display flex;
    //-webkit-flex-flow row wrap;
    //flex-flow row wrap;
    //-webkit-align-items: center;
    //align-items: center;
    position relative;
    font-family: 'fzxs12' !important;
    font-weight: 400;
    font-style: normal;
    font-variant: normal;
    text-transform: none;
    speak: none;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    font-size .36rem;
    color #fe9802;
    width 3.7rem;
    height 2.62rem;
    margin 0 auto .14rem;
    p {
      {$public-center-completely}
      width 100%;
      text-align center;
      span {
        opacity 0;
      }
      .red {
        color #ff1a24;
        font-size .5rem;
      }
    }
    .big .red{
      font-size .55rem;
    }
  }
  .machine-btn{
    display block;
    width 2.54rem;
    height .61rem;
    margin 0 auto;
    &.btn1{
      background url("data:image/png;base64,") no-repeat 0 0;
      background-size 100%;
    }
    &.btn2{
      background url("data:image/png;base64,") no-repeat 0 0;
      background-size 100%;
    }
    &.btn3{
      background url("data:image/png;base64,") no-repeat 0 0;
      background-size 100%;
    }
    &.btn4{
      background url("data:image/png;base64,") no-repeat 0 0;
      background-size 100%;
    }
  }
}
.coupon{
  width 5.8rem;
  margin .42rem auto;
  img {
    display block;
    width 100%;
  }
}
.coupon-btn{
  display block;
  margin 0 auto;
  width 2.5rem;
  border-radius .28rem;
  text-align center;
  font-size .32rem;
  line-height .56rem;
  background-color #ff1a24;
  color #fff
  &.disabled{
    background-color #ccc;
  }
}

.packet-list{
  li {
    position relative;
    box-sizing border-box;
    padding-top .26rem;
    margin .3rem auto;
    width 4.75rem;
    height 1.64rem;
    color #fff;
    background url(../../images/m20161218/packet.png) no-repeat 0 0;
    background-size 100%;

    .limit{
      width 2.94rem;
      height .85rem;
      font-size .3rem;
      line-height .85rem;
      &:before{
        content '';
        opacity 0;
      }
      span {
        font-size .7rem;
        margin 0 .12rem;
      }
    }
    .rule{
      width 2.94rem;
      font-size .2rem;
    }
    .right{
      position absolute;
      top .26rem;
      right 0;
      width 1.8rem;
    }
    .deadline{
      line-height .56rem;
      font-size .22rem;
      color #ab8ff1;
    }
    .packet-btn{
      display block;
      margin 0 auto;
      width 1.5rem;
      height .38rem;
      border .02rem solid #ff1a24;
      border-radius .2rem;
      color #ff1a24;
      line-height .38rem;
      font-size .26rem;
    }
  }
}
.user-wrap{
  margin 0 .3rem;
  height .6rem;
  background-color #e6f7ff;
  border-radius .3rem;
  font-size .24rem;
  line-height .6rem;
  color #333;
  a {
    color #333;
  }
}
$clearfix() {
  \*zoom: 1
&:before,
&:after{
  display: table
  content: ""
}
&:after{
  clear: both
}
}
.gift-list{
  //display -webkit-flex;
  //display flex;
  //-webkit-flex-flow row wrap;
  //flex-flow row wrap;
  //-webkit-justify-content space-between;
  //justify-content space-between;
  $clearfix();
  li {
    &:first-child{
      width 100%;
    }
    float left;
    width 50%;
    margin-bottom .2rem;

    .pic{
      position relative;
      height 1.36rem;
      img {
        {$public-center-completely}
        height 1.05rem;
      }
    }
    .name{
      font-size .22rem;
      line-height .36rem;
      color #ff1a24
    }
    .rule{
      font-size .2rem;
      line-height .34rem;
      color #333
    }
  }
}
.explain{
  margin 0 .3rem .5rem;
  font-size .22rem;
  line-height .33rem;
  color #333;
}
.mask{
  position fixed;
  z-index 10;
  top: 0;
  left: 0;
  width 100%;
  height 100%;
  background-color rgba(0, 0, 0, .7);
  .pop{
    box-sizing border-box;
    {$public-center-completely}
    width 5rem;
    height 5rem;
    padding-top .9rem;
    background url("data:image/png;base64,") no-repeat 0 0;
    background-size 100%;
    .content{
      position relative;
      height 2.06rem;
      font-size .32rem;
      line-height .48rem;
      text-align center;
      color #333;
      margin 0 .4rem;
      p {
        {$public-center-completely}
        width 100%;
      }
    }
    .pop-btn{
      display block;
      width 2.4rem;
      margin 0 auto;
      font-size .32rem;
      color #ff1a24;
      line-height .6rem;
      text-align center;
      border .02rem solid #ff1a24;
      border-radius @line-height;
    }
  }
}
.mask2{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  .pop{
    {$public-center-completely}
    width 1.9rem;
    height .6rem;
    background-color rgba(0,0,0,.7);
    text-align center;
    border-radius @height;
    color #fff;
    font-size .32rem;
    line-height @height;
  }
}
#shareMask {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .7) url("../../images/m20161218/share-mask.png") no-repeat 3rem .3rem;
  background-size: 2.81rem auto;
}