.ca-wrap{
  position fixed;
  top 0;
  left: 0;
  width 100%;
  height 100%;
  z-index 100;
  background url(../../images/loading.gif) no-repeat center center;
  background-color rgba(0, 0, 0, 0.8);
  background-size: 1rem auto;
  .ca-pop,.empty{
    position absolute;
    top 50%;
    left 50%;
    background-color #fff;
    width 5.46rem;
    border-radius .1rem;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
  }
  .empty-cont{
    border-bottom 1px solid #ccc;
    padding: .5rem .45rem;
    font-size: .24rem;
    color: #000;
    text-align center;
  }
  .empty-btn{
    display block;
    text-align center;
    font-size: .24rem;
    line-height .68rem;
    text-decoration none;
  }
  .ca-header{
    padding 0 .28rem;
    height 1.2rem;
    line-height 1.2rem;
    font-size .32rem;
    font-weight: bold;
    color #333;
  }
  .ca-content{
    background-color #f4f4f4;
    border-bottom 1px solid #ccc;
    padding: .30rem .45rem;
    font-size: .24rem;
    color: #000;
    p {
      line-height 1.7;
    }
    .label{
      display: inline-block;
      width: 1.80rem;
      text-align: right;
      color: #666;
    }
  }
  .ca-footer{
    height 0.76rem;
    padding .4rem .28rem;
  }
  .ca-btn{
    float left;
    display block;
    width 2.32rem;
    height .76rem;
    box-sizing border-box;
    text-align center;
    line-height .76rem;
    font-size .32rem;
    text-decoration none;
    border-radius .05rem;
  }
  .changeBtn{
    border 1px solid #666;
    color #666;
    margin-right .2rem;
  }
  .comfirmBtn{
    border 1px solid #f35c3f;
    background-color #f35c3f
    color #fff;
  }
}
.ca-hide{
  display none;
}