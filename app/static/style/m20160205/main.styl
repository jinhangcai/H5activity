.stage .has-peach{
  i {
    opacity 1;
    animation: marker-raise .5s ease-in 1;
    -webkit-animation: marker-raise .5s ease-in 1;
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
  }
}
.jump{
  animation: jump 1s linear 1;
  -webkit-animation: jump 1s linear 1;
}
@keyframes marker-raise {
  from {
    -webkit-transform: scale3d(0,0,1) translate3d(0,0,0);
    transform: scale3d(0,0,1) translate3d(0,0,0);
    opacity 1
  }

  50% {
    -webkit-transform: scale3d(0.9,1.1,1) translate3d(0,-30%,0);
    transform: scale3d(0.9,1.1,1) translate3d(0,-30%,0)
  }

  80% {
    -webkit-transform: scale3d(1.1,.85,1) translate3d(0,0,0);
    transform: scale3d(1.1,.85,1) translate3d(0,0,0)
  }

  to {
    -webkit-transform: scale3d(1,1,1) translate3d(0,0,0);
    transform: scale3d(1,1,1) translate3d(0,0,0)
    opacity 1
  }
}

@keyframes jump {
  from {
    -webkit-transform: scale3d(1,1,1) translate3d(0,0,0);
    transform: scale3d(1,1,1) translate3d(0,0,0)
  }

  20% {
    -webkit-transform: scale3d(1,1,1) translate3d(0,.4rem,0);
    transform: scale3d(1,1,1) translate3d(0,.4rem,0)
  }

  50% {
    -webkit-transform: scale3d(1,1,1) translate3d(0,-.6rem,0);
    transform: scale3d(1,1,1) translate3d(0,-.6rem,0)
  }

  70% {
    -webkit-transform: scale3d(1,1,1) translate3d(0,.3rem,0);
    transform: scale3d(1,1,1) translate3d(0,.3rem,0)
  }
  to {
    -webkit-transform: scale3d(1,1,1) translate3d(0,0,0);
    transform: scale3d(1,1,1) translate3d(0,0,0)
  }
}

@keyframes eye {
  from {
    -webkit-transform: scale3d(1,1,1);
    transform: scale3d(1,1,1)
  }
  44% {
    -webkit-transform: scale3d(1,1,1);
    transform: scale3d(1,1,1)
  }
  45% {
    -webkit-transform: scale3d(1,.1,1);
    transform: scale3d(1,.1,1)
  }
  46% {
    -webkit-transform: scale3d(1,1,1);
    transform: scale3d(1,1,1)
  }
  49% {
    -webkit-transform: scale3d(1,1,1);
    transform: scale3d(1,1,1)
  }
  50% {
    -webkit-transform: scale3d(1,.1,1);
    transform: scale3d(1,.1,1)
  }
  51% {
    -webkit-transform: scale3d(1,1,1);
    transform: scale3d(1,1,1)
  }

  to {
    -webkit-transform: scale3d(1,1,1);
    transform: scale3d(1,1,1)
  }
}
@keyframes bounce{
  from {
    -webkit-transform:  translate3d(0,0,0);
    transform:  translate3d(0,0,0)
  }
  44% {
    -webkit-transform:  translate3d(0,0,0);
    transform:  translate3d(0,0,0)
  }
  45% {
    -webkit-transform:  translate3d(0,-3px,0);
    transform:  translate3d(0,-3px,0)
  }
  46% {
    -webkit-transform:  translate3d(0,0,0);
    transform:  translate3d(0,0,0)
  }
  49% {
    -webkit-transform:  translate3d(0,0,0);
    transform:  translate3d(0,0,0)
  }
  50% {
    -webkit-transform:  translate3d(0,-3px,0);
    transform:  translate3d(0,-3px,0)
  }
  51% {
    -webkit-transform:  translate3d(0,0,0);
    transform:  translate3d(0,0,0)
  }

  to {
    -webkit-transform:  translate3d(0,0,0);
    transform:  translate3d(0,0,0)
  }
}
@keyframes hang {
  from {
      -webkit-transform: rotate(0);
      transform: rotate(0)
    }
  20%{
      -webkit-transform: rotate(-8deg);
      transform: rotate(-8deg)
    }
  40%{
    -webkit-transform: rotate(6deg);
    transform: rotate(6deg)
  }
  60% {
    -webkit-transform: rotate(-4deg);
    transform: rotate(-4deg)
  }
  80% {
      -webkit-transform: rotate(2deg);
      transform: rotate(2deg)
    }
  90% {
    -webkit-transform: rotate(0);
    transform: rotate(0)
  }
  to {
      -webkit-transform: rotate(0);
      transform: rotate(0)
    }
}

