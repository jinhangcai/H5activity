@import "../reset";
@import "../address_moudle";
$clearfix() {
  \*zoom: 1
  &:before,
  &:after{
    display: table
    content: ""
  }
  &:after{
    clear: both
  }
}
$hliagn(){
  transform translate(0, -50%);
  -webkit-transform translate(0, -50%)
}
$vliagn(){
  transform translate(-50%, 0);
  -webkit-transform translate(-50%, 0)
}
$positioncenter(){
  transform translate(-50%, -50%);
  -webkit-transform translate(-50%, -50%)
}
$bg-color = #1e847a;
$bg-sky = #fff7d2;
$high-color = #731f77;
.underline{
  text-decoration none;
  border-bottom 1px solid;
}
.high-color{
  color $high-color;
}
.strong{
  font-weight bold;
}
.fr{
  float right;
}
.par{
  font-size .25rem;
  line-height .33rem;
}
html{
  font-size: 100px;
}
body{
  font-size: 0.14rem;
  line-height: 1.2;
}
html, body{
  -webkit-overflow-scrolling: touch;
  overflow-scrolling: touch;
}
.viewport {
  max-width: 720px;
  margin: 0 auto;
  position: relative;
}
.choice-bg{
  background-color: $bg-sky
}
.stage-bg{
  background-color: $bg-color
}

#choiceTree{
  .tit{
    width 2.64rem;
    height .36rem;
    margin .47rem auto;
    img {
      width 100%;
    }
  }
  .trees-list{
    width 5.62rem;
    margin 0 auto;
    $clearfix();
  }
  .tree-item{
    position relative;
    float left;
    width 1.61rem;
    height 5.15rem;
    margin 0 .13rem;
    &:before{
      content '';
      position absolute;
      left 50%;
      $vliagn();
      bottom .18rem;
      width .45rem;
      height .45rem;
      border .03rem solid #cf3c2e;
      opacity .3;
      border-radius 50%;
    }
    img {
      width 100%;
      height 100%;
    }
  }
  .tree-checked{
    background-color #ffe87e
    &:before{
      opacity 1;
    }
    &:after{
      content '';
      position absolute;
      bottom .27rem;
      left .64rem;
      width .41rem;
      height .34rem;
      background url(../../images/m20160205/tick-icon.png) no-repeat 0 0;
      background-size 100% 100%;
    }
  }
  .btn{
    display block;
    width 2.8rem;
    height .8rem;
    background-color #cf3c29;
    font-size .36rem;
    color #fff;
    text-align center;
    line-height .8rem;
    margin .6rem auto 0;
    border-radius .4rem;
  }
  .dis-btn{
    opacity .3;
    pointer-events none;
  }
  .step-two{
    display none;
    .cont{
      position relative;
      width 3.69rem
      height 5.39rem;
      background url(../../images/m20160205/step-two-bg.png) no-repeat 0 0;
      background-size 100% 100%;
      margin .87rem auto 1.25rem;
      .peach-send{
        position absolute;
        top 3.08rem;
        left 50%;
        $vliagn();
        line-height .93rem;
        font-size .36rem;
        font-weight bold;
        color #cf3c29
        padding-left .9rem;
        background url(../../images/m20160205/peach.png) no-repeat 0 0;
        background-size .72rem .93rem;
      }
    }
  }
}

#stageWrap{
  position relative;
  .top-bar{
    position absolute;
    z-index 2;
    top 0;
    left: 0;
    box-sizing border-box;
    width: 100%;
    height: .9rem;
    padding .36rem .32rem 0;
    .peach-num{
      position relative;
      float left;
      box-sizing border-box;
      min-width 1.4rem;
      line-height .5rem;
      padding 0 .2rem 0 .6rem;
      font-size .28rem;
      color #fff;
      text-align right;
      background-color: #cd3325;
      border-radius: .25rem;
      &:before{
        content '';
        position absolute;
        left -.12rem;
        top: -.06rem;
